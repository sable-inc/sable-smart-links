{
  "name": "sable-smart-links",
  "version": "1.4.64",
  "description": "Product walkthroughs triggered by a specified link, integrated directly with your platform.",
  "homepage": "https://github.com/sable-inc/sable-smart-links#readme",
  "bugs": {
    "url": "https://github.com/sable-inc/sable-smart-links/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sable-inc/sable-smart-links.git"
  },
  "license": "MIT",
  "author": "Sable (Leon Chen)",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "default": "./dist/index.umd.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.js",
      "default": "./dist/react/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "react": [
        "dist/react/index.d.ts"
      ]
    }
  },
  "scripts": {
    "test": "jest",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint src/**/*.js",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.43.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup": "^3.25.1",
    "rollup-plugin-dts": "^6.0.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.826.0",
    "@chakra-ui/react": "^3.19.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "framer-motion": "^12.16.0",
    "openai": "^5.1.1",
    "zod": "^3.25.53"
  }
}
