{
  "name": "sable-smart-links",
  "version": "1.1.0",
  "description": "Product walkthroughs triggered by a specified link, integrated directly with your platform.",
  "homepage": "https://github.com/sable-inc/sable-smart-links#readme",
  "bugs": {
    "url": "https://github.com/sable-inc/sable-smart-links/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sable-inc/sable-smart-links.git"
  },
  "license": "MIT",
  "author": "Sable (Leon Chen)",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.esm.js",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    },
    "./next": {
      "import": "./dist/next.esm.js",
      "require": "./dist/next.js",
      "types": "./dist/next.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "react": [
        "dist/react.d.ts"
      ],
      "next": [
        "dist/next.d.ts"
      ]
    }
  },
  "scripts": {
    "test": "jest",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint src/**/*.js",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.27.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^18.3.22",
    "eslint": "^8.43.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^3.25.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "index.d.ts"
  ],
  "dependencies": {
    "@aws-amplify/auth": "^6.12.4"
  }
}
